{
  "name": "m-doc",
  "version": "0.1.1",
  "description": "A document management tool",
  "main": "bin/m-doc.js",
  "author": {
    "name": "Dong Xing",
    "email": "dong.xing@outlook.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WX-DongXing/m-doc.git"
  },
  "homepage": "https://github.com/WX-DongXing/m-doc.git",
  "keywords": [
    "doc",
    "management",
    "m-doc"
  ],
  "files": [
    "lib/*",
    "public/*",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "m-doc": "bin/m-doc.js"
  },
  "scripts": {
    "client": "cd client && yarn serve",
    "package": "cd client && yarn build",
    "serve": "nodemon bin/m-doc.js -no-browser"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "docs/*"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "fs-extra": "^9.1.0",
    "lowdb": "^1.0.0",
    "markdown-it": "^12.0.4",
    "open": "^8.0.2",
    "sirv": "^1.0.11",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "nodemon": "^2.0.7"
  },
  "engines": {
    "node": ">=12.20.0"
  }
}
